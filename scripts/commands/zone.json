{
  "id": "zone_command",
  "type": "register_command",
  "data": {
    "name": "zones",
    "subcommands": [
      {
        "name": "create",
        "description": "Create a new zone",
        "arguments": [
          {
            "name": "name",
            "type": "string",
            "description": "The name of the zone"
          },
          {
            "name": "size",
            "type": "integer",
            "description": "The size of the zone (side length)"
          },
          {
            "name": "description",
            "type": "string",
            "description": "Description of the zone"
          }
        ]
      },
      {
        "name": "delete",
        "description": "Delete a zone",
        "arguments": [
          {
            "name": "id",
            "type": "string",
            "description": "The ID of the zone to delete"
          }
        ]
      },
      {
        "name": "edit",
        "description": "Edit a zone",
        "arguments": [
          {
            "name": "id",
            "type": "string",
            "description": "The ID of the zone to edit"
          },
          {
            "name": "description",
            "type": "string",
            "description": "New description for the zone"
          },
          {
            "name": "teleport",
            "type": "boolean",
            "description": "Allow teleportation to the zone"
          },
          {
            "name": "sale",
            "type": "boolean",
            "description": "Put the zone up for sale"
          },
          {
            "name": "price",
            "type": "integer",
            "description": "Set the price for the zone"
          }
        ]
      },
      {
        "name": "info",
        "description": "Get information about a zone",
        "arguments": [
          {
            "name": "id",
            "type": "string",
            "description": "The ID of the zone"
          }
        ]
      },
      {
        "name": "list",
        "description": "List all zones"
      }
    ]
  }
}
